<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hsyuan.inventropy.mapper.ProjectLogMapper">
    <insert id="insert">
        insert into tb_project_log(project_id,old_status,new_status,reason,create_time,approver_id) values(#{projectId},#{oldStatus},#{newStatus},#{reason},#{createTime},#{approverId})
    </insert>
</mapper>