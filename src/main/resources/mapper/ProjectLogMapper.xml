<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hsyuan.inventropy.mapper.ProjectLogMapper">
    <insert id="insert">
        insert into tb_project_log(project_id,old_status,new_status,reason,approver_id) values(#{projectId},#{oldStatus},#{newStatus},#{reason},#{approverId})
    </insert>
    <delete id="deleteByProjectId">
        delete from tb_project_log where project_id = #{projectId}
    </delete>
    <select id="getAll" resultType="com.hsyuan.inventropy.entity.ProjectLog">
        select id,project_id as projectId,old_status as oldStatus,new_status as newStatus,reason,create_time as createTime,approver_id as approverId
        from tb_project_log
    </select>
</mapper>